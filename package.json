{
  "name": "cpi",
  "version": "1.0.0",
  "scripts": {
    "build": "npm-run-all clean build:production:*",
    "build:production:client": "yarn webpack --env mode=production --env presets=client",
    "build:production:server": "yarn webpack --env mode=production --env presets=server",
    "clean": "rm -rf dist",
    "dev": "cross-env DEBUG=server:* npm-run-all clean serve:dev",
    "analyze": "cross-env ANALYZE=true cross-env DEBUG=server:* npm-run-all clean serve:dev",
    "start": "cross-env NODE_ENV=production cross-env DEBUG=server:* node dist/server.js",
    "serve:dev": "cross-env NODE_ENV=development ts-node src/server/devServer.ts",
    "webpack": "cross-env NODE_ENV=production webpack",
    "lint": "eslint . --ext .js,.tsx,.ts",
    "lint:fix": "eslint . --fix --ext .js,.tsx,.ts",
    "test": "jest src",
    "test:coverage": "jest src --coverage",
    "prepare": "husky install"
  },
  "author": "Carlos Santana <CarlosSantanaEngineer@gmail.com>",
  "license": "MIT",
  "dependencies": {
    "@apollo/client": "^3.3.21",
    "@contentpi/lib": "^1.0.6",
    "@contentpi/ui-kit": "1.6.13",
    "@testing-library/jest-dom": "^5.14.1",
    "@testing-library/react": "^12.0.0",
    "cookie-parser": "^1.4.5",
    "copy-webpack-plugin": "^9.0.1",
    "cors": "^2.8.5",
    "dotenv-webpack": "^7.0.3",
    "express": "^4.17.1",
    "express-session": "^1.17.2",
    "express-static-gzip": "^2.1.1",
    "graphql": "^15.5.1",
    "html-webpack-plugin": "^5.3.2",
    "isomorphic-fetch": "^3.0.0",
    "jsonwebtoken": "^8.5.1",
    "react": "^17.0.2",
    "react-cookie": "^4.0.3",
    "react-dom": "^17.0.2",
    "react-router-dom": "^5.2.0",
    "run-script-webpack-plugin": "^0.0.11",
    "serialize-javascript": "^6.0.0",
    "styled-components": "^5.3.0",
    "ts-node": "^10.1.0",
    "typescript-plugin-styled-components": "^2.0.0",
    "webpack": "^5.44.0",
    "webpack-bundle-analyzer": "^4.4.2",
    "webpack-dev-server": "^3.11.2",
    "webpack-merge": "^5.8.0",
    "webpack-node-externals": "^3.0.0",
    "webpackbar": "^5.0.0-3"
